# .env  (repo root)
INSTRUMENTS=EURUSD,GBPUSD,USDJPY,AUDUSD,USDCHF,USDCAD,EURJPY,GBPJPY,NZDUSD
DUKA_FROM_MONTH=2023-08
DUKA_TO_MONTH=2025-10
DUKA_TIMEFRAME=m1
DUKA_DISABLE_CLI_FALLBACK=1

# ----- COMMODS (oil etc.) -----
INSTRUMENTS_COMMOD=brent          # comma separated: e.g. brent,wti
DUKA_FROM_MONTH_COMMOD=2023-08
DUKA_TO_MONTH_COMMOD=2025-10
DUKA_TIMEFRAME_COMMOD=m1          # m1 required for 1H/4H/1D aggregation
